      					WRITE_ASCII 0x248 ~BGBRANW~ #8 //  override script
      				END	 
      				PATCH_IF (~%crefile%~ STR_EQ ~branwe5.cre~) AND (GAME_IS ~bgt~) BEGIN
      					WRITE_ASCII 0x248 ~BGBRANW~ #8 //  override script
